#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tomtom/dev/reown/test/starter/node_modules/.pnpm/jsonpath-plus@10.3.0/node_modules/jsonpath-plus/bin/node_modules:/Users/tomtom/dev/reown/test/starter/node_modules/.pnpm/jsonpath-plus@10.3.0/node_modules/jsonpath-plus/node_modules:/Users/tomtom/dev/reown/test/starter/node_modules/.pnpm/jsonpath-plus@10.3.0/node_modules:/Users/tomtom/dev/reown/test/starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tomtom/dev/reown/test/starter/node_modules/.pnpm/jsonpath-plus@10.3.0/node_modules/jsonpath-plus/bin/node_modules:/Users/tomtom/dev/reown/test/starter/node_modules/.pnpm/jsonpath-plus@10.3.0/node_modules/jsonpath-plus/node_modules:/Users/tomtom/dev/reown/test/starter/node_modules/.pnpm/jsonpath-plus@10.3.0/node_modules:/Users/tomtom/dev/reown/test/starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jsonpath-cli.js" "$@"
else
  exec node  "$basedir/../../bin/jsonpath-cli.js" "$@"
fi
